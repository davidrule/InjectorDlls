# TimeInjector configuration (simple INI-style)
# Master_Switch: true/false or 1/0 (global enable)
# Custom_Date_Time: ISO-like string in local time, e.g., 1970-01-01T00:00:00
# Immediate_Mode: true/false or 1/0 (install hooks at start)
# Moving_Mode: progressive | static or 1/0. (1=progressive | 0=static)

Master_Switch = true
Custom_Date_Time = 1970-01-01T00:00:00
Immediate_Mode = true
Moving_Mode = progressive

# logging settings
Process_log = false
Presistant_log = false

# Process_log: when false/0 no timeinject.log is created; when true/1 logs are written.
# Presistant_log: when false/0 each run overwrites the log (no append); when true/1 logs append across runs.